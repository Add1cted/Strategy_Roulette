<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.strategyroulette">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Vertigo" android:screenOrientation="portrait"></activity>
        <activity android:name=".Dust2" android:screenOrientation="portrait"/>
        <activity android:name=".Overpass" android:screenOrientation="portrait"/>
        <activity android:name=".Nuke" android:screenOrientation="portrait"/>
        <activity android:name=".Mirage" android:screenOrientation="portrait"/>
        <activity android:name=".Train" android:screenOrientation="portrait"/>
        <activity android:name=".Inferno" android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>